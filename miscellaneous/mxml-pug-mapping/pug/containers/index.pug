      include hbox.pug

      h2 mx:VBox
      include vbox.pug

      h2 mx:Canvas
      div(class="canvas")
         input(type="text")
         input(type="text")
         input(type="text")

      h2 mx:VDividedBox
      div(class="vDividedBox")
         div(class="vDivider")
            input(type="text")         
         div(class="vDivider")
